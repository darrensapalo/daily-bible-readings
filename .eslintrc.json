{
    "extends": ["plugin:jest/recommended"],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "jest/globals": true
    },
    "rules": {
        "semi": "error"
    },
    "plugins": [
        "jest"
    ]
}
